{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "semi": false,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "importOrder": [],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
